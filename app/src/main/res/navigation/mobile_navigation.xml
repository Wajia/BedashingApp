<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_dashboard">

    <fragment
        android:id="@+id/nav_goods_receiving"
        android:name="com.example.bedashingapp.views.goodsreceiving.GoodsReceivingFragment"
        android:label="Goods Receiving"
        tools:layout="@layout/fragment_goods_receiving" />

    <fragment
        android:id="@+id/nav_dashboard"
        android:name="com.example.bedashingapp.views.dashboard.DashboardFragment"
        android:label="Dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/nav_po_list"
        android:name="com.example.bedashingapp.views.goodsreceiving.purchaseorder.PurchaseOrderListFragment"
        android:label="Purchase Order"
        tools:layout="@layout/fragment_purchase_orders" />

    <fragment
        android:id="@+id/nav_po_item_scan"
