        tools:layout="@layout/fragment_goods_receiving_pdf" />

    <fragment
        android:id="@+id/nav_completed_docs"
        android:name="com.example.bedashingapp.views.completed_documents.CompletedDocumentsFragment"
        android:label="Completed Documents"
        tools:layout="@layout/fragment_documents_completed"/>

    <fragment
        android:id="@+id/nav_doc_payload"
        android:name="com.example.bedashingapp.views.completed_documents.DocumentPayloadFragment"
